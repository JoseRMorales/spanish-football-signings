{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}